<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--=====================================================================-->
  <!--=============================== ants ================================-->
  <!--=====================================================================-->
  <!-- ant -->
  <ThingDef ParentName="OrganicProductBase">
    <defName>I_EMAnt</defName>
    <label>autovim ant</label>
    <description>Indigenous ants that are remarkably passive towards humans. Can be refined down into a mild stimulant drug.</description>
    <graphicData>
      <texPath>Things/Item/ant</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <thingCategories>
      <li>EM_Insects</li>
    </thingCategories>
    <ingestible>
      <foodType>AnimalProduct</foodType>
      <ingestEffect>EatMeat</ingestEffect>
      <ingestSound>RawMeat_Eat</ingestSound>
      <tasteThought>AteRawFood</tasteThought>
      <preferability>DesperateOnly</preferability>
    </ingestible>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <Nutrition>0.005</Nutrition>
      <MaxHitPoints>20</MaxHitPoints>
      <MarketValue>1</MarketValue>
      <Mass>0.025</Mass>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>3</DeteriorationRate>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>2</daysToRotStart>
        <rotDestroys>true</rotDestroys>
        <disableIfHatcher>true</disableIfHatcher>
      </li>
    </comps>
  </ThingDef>
</Defs>